



<h2>Eat-Da-Burger</h2>

<div class="left">
  <ul>
    {{#each burger}}
      {{#unless this.devoured}}
      <p>
        {{this.burger_name }}
      </p>
      <form>
        <button class="change-devoured" type="submit" data-id={{this.id}}>Devoured</button>
      </form>
      {{/unless}}
    {{/each}}
  </ul>
</div>

<h2>test</h2>
<div class="right">
  <ul>
    {{#each burger}}
      {{#if this.devoured}}
      <p>
        {{this.burger_name }}
      </p>
      {{/if}}
    {{/each}}
  </ul>
</div>




<form class="create-form">

  <div class="form-group">
    <input type="text" id="burg" name="name">
  </div>


  <button type="submit">Submit</button>
</form>

<p>Enter a burger you want to eat Click the "Submit" button</p>